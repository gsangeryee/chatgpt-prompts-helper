{
  "manifest_version": 3,
  "name": "Prompts Library",
  "version": "1.0",
  "description": "Chrome extension for chat.openai.com to show a Prompts Library",
  "icons": {
    "48": "icon.png"
  },
  "permissions": ["activeTab", "clipboardWrite", "storage"],
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Prompts Library"
  }
}
